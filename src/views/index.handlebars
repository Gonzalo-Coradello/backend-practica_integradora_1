<div>
    <h1>Products</h1>
    {{#if products}}
        {{#each products}}
            <div>
                <p>{{this.id}}</p>
                <p>{{this.title}}</p>
                <p>{{this.price}}</p>
                <p>{{this.category}}</p>
                <img src="{{this.thumbnails[0]}}">
            </div>
        {{/each}}
    {{else}}
        <h2>No se encontraron productos</h2>
    {{/if}}
</div>